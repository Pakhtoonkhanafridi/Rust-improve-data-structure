celsuis_str = input("Enter temperature in Celsius: ")
celsuis_str = float(celsuis_str)
fahrenheit = (celsuis_str * 9/5) + 32
print(f"Temperature in Fahrenheit: {fahrenheit}")

fahrenheit_str = input("Enter temperature in Fahrenheit: ")
fahrenheit_str = float(fahrenheit_str)
celsuis = (fahrenheit_str - 32) * 5/9
print(f"Temperature in Celsius: {celsuis}")

